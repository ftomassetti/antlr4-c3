{
  "name": "antlr4-c3",
  "version": "1.1.3",
  "description": "A code completion core implmentation for ANTLR4 based parsers",
  "main": "out/index.js",
  "typings": "out/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run test",
    "test": "tsc --version && tsc && mocha out/test",
    "generate": "antlr4ts test/CPP14.g4 test/Expr.g4 test/SandyLexer.g4 -no-listener -no-visitor && cd test && antlr4ts SandyParser.g4 -no-listener -no-visitor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-lischke/antlr4-c3"
  },
  "keywords": [
    "ANTLR4",
    "code completion",
    "auto completion",
    "grammar",
    "parser"
  ],
  "dependencies": {
    "antlr4ts": "^0.4.1-alpha.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.78",
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "path": "^0.12.7",
    "typescript": "~2.3.4"
  },
  "author": "Mike Lischke",
  "license": "MIT"
}
